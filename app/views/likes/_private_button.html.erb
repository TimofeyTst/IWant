<%= turbo_frame_tag "#{dom_id(comment)} private_likes" do %>
    <% image_src = current_user.liked?(comment) ? "favorite_pressed.png" : "favorite.png" %>
    <%= link_to (image_tag image_src), like_comment_path(post_id: post.id, id: comment.id), data: {turbo_method: "post"}, class:"like button" %>
<% end %>    